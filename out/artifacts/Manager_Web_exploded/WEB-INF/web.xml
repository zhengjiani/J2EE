<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <error-page>
        <error-code>500</error-code>
        <location>/error/500.jsp</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/error/404.jsp</location>
    </error-page>
    <servlet>
        <servlet-name>AddManagerServlet</servlet-name>
        <servlet-class>com.zjn.web.AddManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddManagerServlet</servlet-name>
        <url-pattern>/servlet/AddManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListManagerServlet</servlet-name>
        <servlet-class>com.zjn.web.ListManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListManagerServlet</servlet-name>
        <url-pattern>/servlet/ListManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ManagerInfoServlet</servlet-name>
        <servlet-class>com.zjn.web.ManagerInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManagerInfoServlet</servlet-name>
        <url-pattern>/servlet/ManagerInfoServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateManagerServlet</servlet-name>
        <servlet-class>com.zjn.web.updateManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateManagerServlet</servlet-name>
        <url-pattern>/servlet/updateManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DelManagerServlet</servlet-name>
        <servlet-class>com.zjn.web.DelManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DelManagerServlet</servlet-name>
        <url-pattern>/servlet/DelManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BatchDelServlet</servlet-name>
        <servlet-class>com.zjn.web.BatchDelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BatchDelServlet</servlet-name>
        <url-pattern>/servlet/BatchDelServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FindManagerByCondServlet</servlet-name>
        <servlet-class>com.zjn.web.FindManagerByCondServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindManagerByCondServlet</servlet-name>
        <url-pattern>/servlet/FindManagerByCondServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PageManagerServlet</servlet-name>
        <servlet-class>com.zjn.web.PageManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PageManagerServlet</servlet-name>
        <url-pattern>/servlet/PageManagerServlet</url-pattern>
    </servlet-mapping>

</web-app>